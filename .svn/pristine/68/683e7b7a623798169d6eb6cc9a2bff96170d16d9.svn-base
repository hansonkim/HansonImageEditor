<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maekpoong.talktab.imageeditor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        <activity
            android:name="com.moriahtown.imageeditor.ImageEditorActivity">
            <intent-filter>
                <action android:name="com.moriahtown.imageeditor" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.moriahtown.imageeditor.ImageViewActivity"
            android:label="@string/title_activity_image_view" >
        </activity>
    </application>

</manifest>